{
    "type": "module",
    "private": true,
    "scripts": {
        "prestart": "dotnet tool restore",
        "start": "dotnet fable watch ./src/Pages --outDir ./src/Pages/.fable-build --run vite",
        "prebuild": "dotnet tool restore",
        "build:pages": "dotnet fable ./src/Pages --outDir ./src/Pages/.fable-build --run vite build",
        "build:worker": "dotnet fable ./src/Worker --outDir ./dist/worker",
        "build": "npm run build:worker && npm run build:pages"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.19",
        "postcss": "^8.4.38",
        "vite": "^5.2.6"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.6.0",
        "daisyui": "^4.12.22",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwindcss": "^3.4.1",
        "use-sync-external-store": "^1.6.0"
    }
}
